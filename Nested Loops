

#include <iostream>
using namespace std;

int main(){
	for (int i = 0; i < 3; i++)
		for (int j = 0; j < 3; j++){
			cout << endl;
			cout << "***";
			
		}
	cout << endl;
}





//2.)


#include <iostream>
using namespace std;

int main(){
	for (int i = 0; i < 2; i++)
		for (int j = 0; j < 2; j++){
			cout << endl;
			cout << "*********";
			
		}
	cout << endl;
}

//3.)

#include <iostream>
using namespace std;

int main(){
	for (int i = 0; i < 1; i++)
		for (int j = 0; j < 1; j++){
			cout << endl;
			cout << "000000000" << endl;
			cout << "111111111" << endl;
			cout << "222222222" << endl;
			cout << "333333333" << endl;

			
		}
	cout << endl;
}

//4.)

#include <iostream>
using namespace std;

int main(){
	for (int i = 0; i < 1; i++)
			
		for (int j = 0; j < 7; j++){
			cout << endl;
			cout << "0";
			cout << "1";
			cout << "2";

			
		}
	cout << endl;
}

