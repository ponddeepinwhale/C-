
#include <iostream>
using namespace std;


int main()
{
    
    choice = "donut";
    while (choice != "quit");
    print(choice);
    choice = input("Type y to print donut again.")
        if (input == 'Y')
            cout << "Donut" << endl;
        else
            choice = "quit" << endl;


    for (int i = 4; i <= 44; i += 5)
        cout << i << "";
    cout << endl;
    for (int j = 100; j >= 20; j -= 10)
        cout << j << "";
    cout << endl;
    for (int h = 2; h <= 200; h *= 2)
        cout << h << "";
    cout << endl;

    




}
